[
	{
		"key": "cmd+numpad4",
		"command": "editor.action.moveCarretLeftAction",
		"when": "editorHasSelection"
	},
	{
		"key": "cmd+numpad6",
		"command": "editor.action.moveCarretRightAction",
		"when": "editorHasSelection"
	},
	{
		"key": "cmd+numpad4",
		"command": "editor.action.moveCarretLeftAction",
		"when": "editorHasMultipleSelection"
	},
	{
		"key": "cmd+numpad6",
		"command": "editor.action.moveCarretRightAction",
		"when": "editorHasMultipleSelection"
	},
	// MOVE CARRET
	{
		"key": "alt+left",
		"command": "editor.action.moveCarretLeftAction",
		"when": "editorHasSelection"
	},
	{
		"key": "alt+right",
		"command": "editor.action.moveCarretRightAction",
		"when": "editorHasSelection"
	},
	{
		"key": "alt+left",
		"command": "editor.action.moveCarretLeftAction",
		"when": "editorHasMultipleSelection"
	},
	{
		"key": "alt+right",
		"command": "editor.action.moveCarretRightAction",
		"when": "editorHasMultipleSelection"
	},
	{
		"key": "cmd+numpad_multiply",
		"command": "workbench.action.positionPanelBottom",
		"when": "panelPosition == right"
	},
	{
		"key": "cmd+numpad_multiply",
		"command": "workbench.action.positionPanelRight",
		"when": "panelPosition == bottom"
	},
	{
		"key": "shift+cmd+6",
		"command": "workbench.action.positionPanelRight"
	},
	{
		"key": "shift+cmd+7",
		"command": "workbench.action.positionPanelBottom"
	},
	{
		"key": "ctrl+u",
		"command": "editor.action.transformToUppercase"
	},
	{
		"key": "ctrl+l",
		"command": "editor.action.transformToLowercase"
	},
	{
		"key": "cmd+left",
		"command": "explorer.openAndPassFocus",
		"when": "sideBarFocus && activeViewlet == 'workbench.view.explorer'"
	},
	{
		"key": "cmd+right",
		"command": "explorer.openToSide",
		"when": "sideBarFocus && activeViewlet == 'workbench.view.explorer'"
	},
	{
		"key": "alt+tab alt+left",
		"command": "editor.fold",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "alt+cmd+[",
		"command": "-editor.fold",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "alt+tab alt+right",
		"command": "editor.unfold",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "alt+cmd+]",
		"command": "-editor.unfold",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "tab",
		"command": "-acceptSelectedSuggestion",
		"when": "suggestWidgetVisible && textInputFocus"
	},
	{
		"key": "alt+shift+[",
		"command": "editor.action.insertSnippet",
		"when": "editorTextFocus",
		"args": {
			"snippet": "{ ${TM_SELECTED_TEXT} }"
		}
	},
	{
		"key": "alt+shift+7",
		"command": "editor.action.insertSnippet",
		"when": "editorTextFocus",
		"args": {
			"snippet": "&(${TM_SELECTED_TEXT})"
		}
	},
	{
		"key": "alt+shift+8",
		"command": "editor.action.insertSnippet",
		"when": "editorTextFocus",
		"args": {
			"snippet": "(*${TM_SELECTED_TEXT})"
		}
	},
	{
		"key": "alt+3",
		"command": "extension.enumerate",
		"when": "editorTextFocus"
	},
	{
		"key": "alt+cmd+e",
		"command": "-extension.enumerate",
		"when": "editorTextFocus"
	},
	{
		"key": "alt+r",
		"command": "randomeverything.int"
	},
	{
		"key": "cmd+n",
		"command": "explorer.newFile"
	},
	{
		"key": "cmd+w",
		"command": "workbench.action.terminal.kill",
		"when": "terminalFocus"
	},

]


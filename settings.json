{
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.renderWhitespace": "all",
	"files.trimTrailingWhitespace": true,
	"files.associations": {
		"*.h": "c",
		"*.mix": "c"
	},
	//"python.pythonPath": "C:\\Program Files (x86)\\Microsoft Visual Studio\\Shared\\Python36_64\\python.exe",
	"workbench.colorTheme": "Visual Studio Dark",
	"workbench.colorCustomizations": {
		"activityBar.foreground": "#ffffff",
		"activityBar.background": "#000000",
		"activityBarBadge.background": "#3cfff5e1",
		"activityBarBadge.foreground": "#000000",
		"sideBar.background": "#141414",
		"sideBar.foreground": "#dddddd",
		"sideBarTitle.foreground": "#ffffff",
		"sideBarSectionHeader.background": "#2e2e2e",
		"sideBarSectionHeader.foreground": "#ffffff",
		"editorGroup.border": "#ffffffa1",
		"editorGroupHeader.tabsBackground": "#2e2e2e",
		"editorGroupHeader.tabsBorder": "#ffffffa1",
		"tab.activeBackground": "#464646",
		"editor.foreground": "#ffffff",
		"editor.background": "#1b1b1b",
		"editorLineNumber.foreground": "#ffffffb6",
		"editorLineNumber.activeForeground": "#43fda0",
		"editorCursor.foreground": "#43fda0",
		"editor.selectionBackground": "#7c98b862",
		"editor.selectionHighlightBackground": "#8d35ffa2",
		"editorOverviewRuler.selectionHighlightForeground": "#8d35ffa2",
		"editorBracketMatch.background": "#d243fd52",
		"editorBracketMatch.border": "#00e1ff7c",
		"editorOverviewRuler.bracketMatchForeground": "#af73fdb2",
		"editorError.foreground": "#ff0000",
		"editorWarning.foreground": "#ffbb00",
		"focusBorder": "#ffffff8e",
		"foreground": "#a1a1a1",
		"selection.background": "#3a3a3a69",
		"errorForeground": "#ff0000",
		"contrastBorder": "#9c9c9c",
		"terminal.ansiBlack": "#2e3436",
		"terminal.ansiBlue": "#3465a4",
		"terminal.ansiBrightBlack": "#555753",
		"terminal.ansiBrightBlue": "#729fcf",
		"terminal.ansiBrightCyan": "#34e2e2",
		"terminal.ansiBrightGreen": "#8ae234",
		"terminal.ansiBrightMagenta": "#ad7fa8",
		"terminal.ansiBrightRed": "#f49797",
		"terminal.ansiBrightWhite": "#fbfbfb",
		"terminal.ansiBrightYellow": "#4ffc89",
		"terminal.ansiCyan": "#06989a",
		"terminal.ansiGreen": "#4e9a06",
		"terminal.ansiMagenta": "#75507b",
		"terminal.ansiRed": "#ff0000",
		"terminal.ansiWhite": "#b5bbae",
		"terminal.ansiYellow": "#00ffdd",
		"terminal.foreground": "#ffffff"
	},
	"editor.tokenColorCustomizations": {
		"comments": "#6b6b6b",
		"variables": "#6ae1ff",
		"numbers": "#00ffff",
		"keywords": "#d400ff",
		"functions": "#ff30b0",
		"strings": "#94e4c5",
		"types": "#d400ff"
	},
	"workbench.activityBar.visible": true,
	"editor.minimap.enabled": false,
	"explorer.confirmDelete": false,
	"workbench.sideBar.location": "left",
	"editor.showFoldingControls": "always",
	"editor.detectIndentation": false,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.letterSpacing": 1,
	"terminal.integrated.scrollback": 2000,
	"C_Cpp.updateChannel": "Insiders",
	"workbench.editor.enablePreview": false,
	"window.zoomLevel": -1,
	"cSpell.userWords": [
		"REPOLARIZATION",
		"abelian",
		"clade",
		"nums",
		"parallelization",
		"pthreads",
		"rasterisation",
		"unviable"
	],
	"C_Cpp.default.includePath": [
		"~/.brew/include/**"
	],

	"todo-tree.tree.showScanModeButton": false,

	"todo-tree.general.tagGroups": {
	},

	"todo-tree.highlights.defaultHighlight": {
		"icon": "alert",
		"type": "text",
		"foreground": "yellow",
		"background": "#6f00ff",
		"opacity": 60,
		"iconColour": "yellow"
	},
	"todo-tree.general.tags":[
		"BUG",
		"NOTE",
		"HACK",
		"FIXME",
		"TODO",
		"XXX"
	],
	"terminal.integrated.shell.osx": "/bin/bash",
	"editor.fontSize": 14,
	"terminal.integrated.fontWeightBold": "normal",
	"terminal.external.linuxExec": "iterm",
	"terminal.external.osxExec": "iTerm.app",
	"workbench.fontAliasing": "auto",
	"C_Cpp.default.intelliSenseMode": "clang-x86",
	"editor.quickSuggestionsDelay": 7,
	"editor.suggest.localityBonus": true,
	"editor.tabCompletion": "off",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"C_Cpp.suggestSnippets": false,
	"editor.acceptSuggestionOnEnter": "smart",
	"editor.accessibilitySupport": "on",
	"psi-header.variables": [

	],
	"psi-header.config": {
		"forceToTop": true,
		"author": "Kevin Colour",
		"blankLinesAfter": 1,
		"authorEmail": "kecolour@gmail.com"
	},
	"psi-header.lang-config": [
		{
			"language": "c",
			"begin": "/***************************************************************************",
			"end":   "***************************************************************************/",
			"prefix": " * ",
			"lineLength": 80
		}
	],
	"psi-header.templates": [
		{
			"language": "c",
			"template": [
				"FILENAME:    <<filename>>",
				"DESCRIPTION:",
				"    */",
				"ENVIRONMENT:",
				"    macOS Cataline 10.15.7",
				"    Visual Studio Code 1.30",
				"AUTHORS:",
				"    <<author>>",
				"DATES:",
				"    Created: <<dateformat('DDMMMYYYY')>>"
			]
		}
	]
	//"window.menuBarVisibility": "default"
}
